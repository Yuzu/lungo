{
  "hosting": {
    "public": "./",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/benchmark1",
        "destination": "/b1.html"
      },
      {
        "source": "/benchmark2",
        "destination": "/benchmark2/src/index.html"
      },
      {
        "source": "/benchmark3",
        "destination": "/benchmark3/src/index.html"
      }
    ],
    "headers": [ 
      {
        "source": "*",
        "headers": [ {
          "key": "X-Content-Type-Options",
          "value": ""
        }]
      }
    ]
  }
}